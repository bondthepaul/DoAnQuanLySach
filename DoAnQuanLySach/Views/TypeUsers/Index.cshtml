@model IEnumerable<DoAnQuanLySach.Models.TypeUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Danh sách người dùng </h2>

<p>
    @Html.ActionLink("Tạo người dùng mới", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Loại người dùng
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeName)
        </td>
        <td>
            @Html.ActionLink("Chỉnh sửa", "Edit", new { id=item.TypeId }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.TypeId }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.TypeId })
        </td>
    </tr>
}

</table>
